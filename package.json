{
  "name": "honie-app",
  "version": "1.0.0",
  "main": "dist/backend/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "concurrently \"yarn run start:electron\" \"yarn run start:nest\"",
    "start:electron": "tsc && electron ./dist/electron-main.js",
    "start:nest": "nodemon --watch src/backend --exec \"npx ts-node -r tsconfig-paths/register ./src/backend/main.ts\""
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.2.6",
    "@nestjs/testing": "^7.6.0",
    "@types/dotenv": "^8.2.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^29.5.12",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.14.1",
    "@types/supertest": "^2.0.11",
    "concurrently": "^8.2.2",
    "electron-reload": "^2.0.0-alpha.1",
    "nodemon": "^3.1.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^10.3.9",
    "@nestjs/platform-express": "^10.3.9",
    "bson": "^6.7.0",
    "dotenv": "^8.2.0",
    "electron": "^30.0.9",
    "mongodb": "^6.7.0",
    "mongoose": "^5.10.18",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  }
}
